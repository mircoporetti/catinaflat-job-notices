# Catinaflat credentials
CATINAFLAT_EMAIL=your_email@example.com
CATINAFLAT_PASSWORD=your_password
CATINAFLAT_USER_ID=0123456

# Browser settings
# Set to 'true' for headless mode, 'false' for headed mode
HEADLESS=true

# Email notification settings
# For Gmail, you might need to use an App Password instead of your regular password
# Generate one at: https://myaccount.google.com/apppasswords
EMAIL_SENDER_USER=your_email_app_password_here
EMAIL_SENDER_PASSWORD=your_email_app_password_here
